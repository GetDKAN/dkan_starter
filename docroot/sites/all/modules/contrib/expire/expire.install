/**
 * @file
 * Uninstall actions for Cache Expiration module.
 */

/**
 * Implements hook_uninstall().
 */
function expire_uninstall() {

  db_delete('variable')
    ->condition('name', 'expire_%', 'LIKE')
    ->execute();

  cache_clear_all('variables', 'cache_bootstrap');
}

